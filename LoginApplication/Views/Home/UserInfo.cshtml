@model List<SignUp>
@{
    ViewData["Title"] = "UserInfo";
}

@*<a class="btn btn-outline-primary" asp-controller="Home" asp-action="Create">Create</a>*@
<table class="table table-bordered table-striped">
 <tr>
        <th>User Name</th>
        <th>User Mail</th>
        <th>User Password</th>
        
 </tr>
 
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.UserName</td>
                <td>@item.Email</td>
                <td>@item.NewPassword</td>
            </tr>
        }

</table>

